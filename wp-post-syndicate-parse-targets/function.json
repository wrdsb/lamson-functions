{
    "bindings": [
        {
            "name": "req",
            "type": "httpTrigger",
            "authLevel": "function",
            "direction": "in",
            "methods": [
                "post"
            ]
        },
        {
            "name": "postObject",
            "type": "blob",
            "path": "wp-posts/{httpTrigger}",
            "connection": "wrdsblamson_STORAGE",
            "direction": "in"
        },
        {
            "name": "$return",
            "type": "http",
            "direction": "out"
        }
    ],
    "disabled": false
}